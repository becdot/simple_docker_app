{
  "rules": {
    "curly": 2,
    "camelcase": 0,
    "dot-notation": 2,
    "eqeqeq": [2, "smart"],
    "new-cap": 2,
    "no-caller": 2,
    "no-cond-assign": 2,
    "no-console": 1,
    "no-script-url": 2,
    "no-shadow": 0,
    "no-undef": 2,
    "no-underscore-dangle": 0,
    "no-unused-expressions": [2, {"allowShortCircuit": true, "allowTernary": true}],
    "no-unused-vars": [ 2, {"vars": "all", "args": "none"}],
    "quotes": 0,
    "semi": 2,
    "vars-on-top": 0,
    "no-use-before-define": 0
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "globals": {
    "require": false,
    "process": false
  },
  "extends": "eslint:recommended"
}
